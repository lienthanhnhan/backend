{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": { "^.+\\.(css|less|gif|jpg|jpeg|svg|png)$": "module.exports = {};", "src/(.*)": "<rootDir>/$1" },
  "rootDir": "../src",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "./coverage",
  "testEnvironment": "node"
}
